{
  "manifest_version": 2,
  "name": "Modify CSS and HTML on Beatport",
  "version": "0.1.0",
  "description": "Multi store player Beatport extension",
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "content.js"
      ],
      "matches": [
        "https://beatport.com/*",
        "https://www.beatport.com/*"
      ]
    }
  ]
}
